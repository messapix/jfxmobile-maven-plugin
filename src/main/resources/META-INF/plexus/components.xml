<?xml version="1.0" encoding="UTF-8"?>
<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>mobile</role-hint>
            <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
            <configuration>
                <phases>      
                    <process-resources>org.apache.maven.plugins:maven-resources-plugin:2.7:resources</process-resources>
                    <compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:main-compile</compile>
                    <process-test-resources>org.apache.maven.plugins:maven-resources-plugin:2.7:testResources</process-test-resources>
                    <test-compile>org.apache.maven.plugins:maven-compiler-plugin:3.5:testCompile</test-compile>
                    <test>org.apache.maven.plugins:maven-surefire-plugin:2.19.1:test</test>
                    <package>org.apache.maven.plugins:maven-jar-plugin:2.6:jar</package>
                    <install>org.apache.maven.plugins:maven-install-plugin:2.5.2:install</install>
                    <deploy>org.apache.maven.plugins:maven-deploy-plugin:2.8.2:deploy</deploy>
                </phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>mobile</role-hint>
            <implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler</implementation>
            <configuration>
                <extension>jar</extension>
                <type>mobile</type>
                <packaging>mobile</packaging>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>android-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>android</id>
                <phases>
                    <phase>android-configure</phase>
                    <phase>android-resources</phase>
                    <phase>android-compile</phase>
                    <phase>android-retrolambda</phase>
                    <phase>android-package</phase>
                    <phase>android-merge</phase>
                    <phase>android-manifest</phase>
                    <phase>android-manifestkeep</phase>
                    <phase>android-shrink</phase>
                    <phase>android-maindexlist</phase>
                    <phase>android-dex</phase>
                    <phase>android-process-resources</phase>
                    <phase>android-apk</phase>
                    <phase>android</phase>
                    <phase>android-device</phase>
                </phases>
                <default-phases>
                    <android-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-configure</android-configure>
                    <android-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-resources</android-resources>
                    <android-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-compile</android-compile>
                    <android-retrolambda>
                        com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-retrolambda-main,
                        com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-retrolambda-android,
                        com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-retrolambda-dependencies
                    </android-retrolambda>
                    <android-package>
                        com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-package-main,
                        com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-package-android
                    </android-package>
                    <android-merge>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-singlejar</android-merge>
                    <android-manifest>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-manifest</android-manifest>
                    <android-manifestkeep>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-manifestkeep</android-manifestkeep>
                    <android-shrink>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-shrink</android-shrink>
                    <android-maindexlist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-maindexlist</android-maindexlist>
                    <android-dex>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-dex</android-dex>
                    <android-process-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-process-resources</android-process-resources>
                    <android-apk>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-apk</android-apk>
                    <android-device>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:android-device</android-device>
                </default-phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>desktop-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>desktop</id>
                <phases>
                    <phase>desktop-configure</phase>
                    <phase>desktop-process-resources</phase>
                    <phase>desktop-compile</phase>
                    <phase>desktop-package</phase>
                    <phase>desktop-dist</phase>
                    <phase>desktop</phase>
                </phases>
                <default-phases>
                    <desktop-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:desktop-configure</desktop-configure>
                    <desktop-process-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:desktop-resources</desktop-process-resources>
                    <desktop-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:desktop-compile</desktop-compile>
                    <desktop-package>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:desktop-package</desktop-package>
                    <desktop-dist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:desktop-dist</desktop-dist>
                </default-phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>ios-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>ios</id>
                <phases>
                    <phase>ios-configure</phase>
                    <phase>ios-compile</phase>
                    <phase>ios-launcher</phase>
                    <phase>ios-infoplist</phase>
                    <phase>ios-resources</phase>
                    <phase>ios-ipa</phase>
                    <phase>ios</phase>
                </phases>
                <default-phases>
                    <ios-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-configure</ios-configure>
                    <ios-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-compile</ios-compile>
                    <ios-launcher>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-launcher</ios-launcher>
                    <ios-infoplist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-infoplist</ios-infoplist>
                    <ios-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-resources</ios-resources>
                    <ios-ipa>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-ipa</ios-ipa>
                </default-phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>ios-simulator-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>ios</id>
                <phases>
                    <phase>ios-simulator-configure</phase>
                    <phase>ios-simulator-compile</phase>
                    <phase>ios-simulator-assets</phase>
                    <phase>ios-simulator-resources</phase>
                    <phase>ios-simulator-launcher</phase>
                    <phase>ios-simulator-infoplist</phase>
                    <phase>ios-simulator</phase>
                </phases>
                <default-phases>
                    <ios-simulator-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-simulator-configure</ios-simulator-configure>
                    <ios-simulator-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-compile</ios-simulator-compile>
                    <ios-simulator-launcher>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-launcher</ios-simulator-launcher>
                    <ios-simulator-assets>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-assets</ios-simulator-assets>
                    <ios-simulator-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-resources</ios-simulator-resources>
                    <ios-simulator-infoplist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-infoplist</ios-simulator-infoplist>
                    <ios-simulator>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-simulator</ios-simulator>
                </default-phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>ios-device-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>ios</id>
                <phases>
                    <phase>ios-device-configure</phase>
                    <phase>ios-device-compile</phase>
                    <phase>ios-device-assets</phase>
                    <phase>ios-device-resources</phase>
                    <phase>ios-device-launcher</phase>
                    <phase>ios-device-infoplist</phase>
                    <phase>ios-device</phase>
                </phases>
                <default-phases>
                    <ios-device-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-device-configure</ios-device-configure>
                    <ios-device-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-compile</ios-device-compile>
                    <ios-device-launcher>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-launcher</ios-device-launcher>
                    <ios-device-assets>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-assets</ios-device-assets>
                    <ios-device-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-resources</ios-device-resources>
                    <ios-device-infoplist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-infoplist</ios-device-infoplist>
                    <ios-device>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:ios-device</ios-device>
                </default-phases>
            </configuration>
        </component>
        
        <component>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <role-hint>embedded-lifecycle</role-hint>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>embedded</id>
                <phases>
                    <phase>embedded-configure</phase>
                    <phase>embedded-process-resources</phase>
                    <phase>embedded-compile</phase>
                    <phase>embedded-package</phase>
                    <phase>embedded-dist</phase>
                    <phase>embedded</phase>
                    <phase>embedded-deploy</phase>
                    <phase>embedded-run</phase>
                </phases>
                <default-phases>
                    <embedded-configure>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-configure</embedded-configure>
                    <embedded-process-resources>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-resources</embedded-process-resources>
                    <embedded-compile>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-compile</embedded-compile>
                    <embedded-package>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-package</embedded-package>
                    <embedded-dist>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-dist</embedded-dist>
                    <embedded-deploy>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-deploy</embedded-deploy>
                    <embedded-run>com.messapix.ftatr.jfxmobile:jfxmobile-maven-plugin:embedded-run</embedded-run>
                </default-phases>
            </configuration>
        </component>
    </components>
</component-set>
